{
  "name": "in2code/powermail_cleaner",
  "version": "2.0.0",
  "description": "Powermail Cleaner helps to clean up quick and easy old requests",
  "keywords": [
    "typo3",
    "form",
    "mailform",
    "spamprevention"
  ],
  "authors": [
    {
      "name": "Oliver Eglseder",
      "email": "oliver.eglseder@in2code.de"
    },
    {
      "name": "Stefan Busemann",
      "email": "stefan.busemann@in2code.de"
    }
  ],
  "type": "typo3-cms-extension",
  "license": "GPL-2.0-or-later",
  "require": {
    "typo3/cms-core": "^10.4",
    "in2code/powermail": "^8.4",
    "ext-json": "*",
    "ext-gd": "*"
  },
  "require-dev": {
    "typo3/cms-about": "^10.4",
    "typo3/cms-adminpanel": "^10.4",
    "typo3/cms-backend": "^10.4",
    "typo3/cms-belog": "^10.4",
    "typo3/cms-beuser": "^10.4",
    "typo3/cms-extbase": "^10.4",
    "typo3/cms-extensionmanager": "^10.4",
    "typo3/cms-filelist": "^10.4",
    "typo3/cms-fluid": "^10.4",
    "typo3/cms-fluid-styled-content": "^10.4",
    "typo3/cms-form": "^10.4",
    "typo3/cms-frontend": "^10.4",
    "typo3/cms-impexp": "^10.4",
    "typo3/cms-info": "^10.4",
    "typo3/cms-install": "^10.4",
    "typo3/cms-lowlevel": "^10.4",
    "typo3/cms-opendocs": "^10.4",
    "typo3/cms-recordlist": "^10.4",
    "typo3/cms-recycler": "^10.4",
    "typo3/cms-redirects": "^10.4",
    "typo3/cms-reports": "^10.4",
    "typo3/cms-rte-ckeditor": "^10.4",
    "typo3/cms-scheduler": "^10.4",
    "typo3/cms-seo": "^10.4",
    "typo3/cms-setup": "^10.4",
    "typo3/cms-tstemplate": "^10.4",
    "typo3/cms-viewpage": "^10.4",
    "helhum/typo3-console": "^6.7"
  },

  "autoload": {
    "psr-4": {
      "In2code\\PowermailCleaner\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web",
      "extension-key": "powermail_cleaner"
    }
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/powermail_cleaner ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/powermail_cleaner",
      "@package-states"
    ],
    "package-states": [
      ".Build/bin/typo3cms install:generatepackagestates",
      ".Build/bin/typo3cms install:fixfolderstructure"
    ]
  }
}
